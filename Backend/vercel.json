{
  "version": 2,
  "builds": [
    {
      "src": "../frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "../frontend/public"
      }
    },
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "index.js"
    },
    {
      "src": "/(.*)",
      "dest": "../frontend/public/index.html"
    }
  ]
}
